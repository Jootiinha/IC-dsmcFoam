FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      dsmcInitialiseDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
configurations
(
    configuration
    {
        type                    dsmcMeshFill;

		numberDensities
		{
			O2 			1e19;
		};

		translationalTemperature     10000;
		rotationalTemperature     10000;
		vibrationalTemperature     10000;
        electronicTemperature       10000;

		velocity        (0 0 0);
    }
);

